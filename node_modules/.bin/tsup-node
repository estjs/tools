#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules/tsup/dist/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules/tsup/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/tools/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules:/Users/xd/Documents/www/tools/node_modules/tsup/dist/node_modules:/Users/xd/Documents/www/tools/node_modules/tsup/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules/tsup/dist/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules/tsup/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/tsup@6.1.2/node_modules:/Users/xd/Documents/www/tools/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/tools/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules:/Users/xd/Documents/www/tools/node_modules/tsup/dist/node_modules:/Users/xd/Documents/www/tools/node_modules/tsup/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
